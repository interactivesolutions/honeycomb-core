{
  "name": "interactivesolutions/honeycomb-core",
  "description": "HoneyComb CMS package for backend elements",
  "keywords": ["honeycomb", "honey", "laravel"],
  "homepage": "https://github.com/interactivesolutions/honeycomb-core/",
  "license": "MIT",
  "authors": [
    {
      "name": "Mantas Paulauskas",
      "email": "mp@interactivesolutions.lt"
    },
    {
      "name": "Jevgenij Dmitrijev",
      "email": "jd@interactivesolutions.lt"
    },
    {
      "name": "Vytautas Rimeikis",
      "email": "vytautas.rimeikis@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel\/framework": "5.3.*",
    "ramsey\/uuid": "^3.5",
    "jenssegers/rollbar": "*",
    "rap2hpoutre/laravel-log-viewer": "*",
    "spatie/laravel-cookie-consent": "1.*"
  },
  "require-dev": {
    "brianium/paratest": "^0.14",
    "mockery/mockery": "^0.9",
    "orchestra/testbench-browser-kit": "^3.3",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "InteractiveSolutions\\HoneycombCore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "test": [
      "paratest -p4 --colors"
    ]
  },
  "config": {
    "secure-http": false,
    "platform": {
      "php": "7.1"
    }
  }
}
